    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.AspNetCore.Cryptography.Internal": "1.2.0-*",
    "Microsoft.AspNetCore.DataProtection.Test.Shared": {
      "type": "build",
      "version": "1.0.0-*"
    },
    "Microsoft.AspNetCore.Testing": "1.2.0-*",
    "xunit": "2.2.0-*"
  },
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.2.0-*",
